[connection]
id=$WORK_WIFI_SSID
uuid=c2916d12-b7b6-44ec-aa76-2ef05bec0273
type=wifi
permissions=

[wifi]
mac-address-blacklist=
mode=infrastructure
ssid=$WORK_WIFI_SSID

[wifi-security]
key-mgmt=wpa-eap

[802-1x]
eap=peap;
identity=$LDAP_USER
password=$LDAP_PASSWORD
phase2-auth=mschapv2

[ipv4]
dns-search=
method=auto

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto

[proxy]
